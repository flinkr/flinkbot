{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "tabs"],
        "no-console": [false, "log", "error"],
        "object-literal-sort-keys": [false, "warning"],
        "object-literal-key-quotes": [false, "warning"],
        "ordered-imports": [false, "warning"]
    },
    "rulesDirectory": []
}