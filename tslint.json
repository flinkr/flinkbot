{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "tabs"],
        "no-console": [false],
        "object-literal-sort-keys": [false],
        "object-literal-key-quotes": [false],
        "ordered-imports": [false],
        "max-line-length": [false],
        "typedef": [true, "call-signature", "parameter", "member-variable-declaration"]
    },
    "rulesDirectory": []
}